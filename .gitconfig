[core]
    editor = vi
[user]
    name = Bob Van Dell
    email = awesomebobx64@gmail.com
[merge]
    tool = opendiff
[push]
    default = current
[pull]
    rebase = true
[rerere]
    enabled = true
[colur]
    ui = true
[alias]
    s = status -s
    lg = log --oneline --decorate=no --graph
    co = checkout
    st = status
    ci = commit
    pu = push -u
[diff]
    renameLimit = 1920
[credential]
    helper = osxkeychain
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
